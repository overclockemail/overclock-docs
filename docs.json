{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Overclock",
  "colors": {
    "primary": "#005FCE",
    "light": "#2073D3",
    "dark": "#003B80"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "changelog"
            ]
          }
        ]
      }],
    "global": {
      "anchors": [
        {
          "anchor": "Book A Demo",
          "href": "https://calendly.com/overclock/overclock-demo",
          "icon": "calendar"
        },
        {
          "anchor": "Blog",
          "href": "https://overclock.email/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/dark.svg",
    "dark": "/logo/light.svg"
